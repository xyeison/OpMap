# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Server-side environment variables (for API routes)
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Service Role Key - MANTENER SECRETO, NO COMPARTIR
# Obtén este key desde Supabase: Settings → API → service_role
# Este key es necesario para operaciones administrativas que bypasean RLS
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Google Maps API (for client-side maps and distance calculations)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here